{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^http://127.0.0.1"
    },
    {
      "pattern": "^(https?://)?([a-zA-Z0-9-]+\\.)*bilibili\\.com"
